<root>
    <port>8110</port>
    <device>Chiller
        <use>true</use>
        <name>chiller</name>
        <value>Coolant
            <period>2</period>
            <enabled>true</enabled>
        </value>
    </device>
    <device>AirPressure
        <name>air_transducer</name>
        <value>pressure
            <period>on_change</period>
            <enabled>true</enabled>
        </value>
        <use>true</use>
    </device>
    <device>BoneGauges
        <name>bone_micro_ion_controller</name>
        <value>IG_pressure
            <func>get_ion_pressure</func>
            <period>on_change</period>
            <enabled>true</enabled>
        </value>
        <value>CG1_pressure
            <func>get_convectron_a_pressure</func>
            <period>2</period>
            <enabled>true</enabled>
        </value>
        <use>false</use>
    </device>
    <device>EnvironmentalMonitor
        <name>enivronmental_monitor</name>
        <value>temperature
            <func>get_temperature</func>
            <period>2</period>
            <enabled>true</enabled>
        </value>
        <value>humidity
            <func>get_humidity</func>
            <period>10</period>
            <enabled>true</enabled>
        </value>
        <use>false</use>
    </device>
</root>